    "cypress": "^14.2.1",
    "depcheck": "^1.4.0",
    "http-server": "^14.1.0",
    "jsdoc": "^4.0.0",
    "jsdom": "^23.0.0",
    "msw": "^2.0.0",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^5.9.0",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "3.4.1",
    "tempo-devtools": "^2.0.106",
    "terser": "^5.39.2",
    "typescript": "^5.2.2",
