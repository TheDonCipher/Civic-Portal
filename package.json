{
  "name": "civic-portal-botswana",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:no-check": "vite build",
    "build-no-errors": "tsc --noEmitOnError false && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "docs:generate": "jsdoc -c jsdoc.config.js",
    "docs:serve": "npx http-server docs/api -p 8080",
    "analyze:bundle": "vite build --config vite.config.bundle-analyzer.ts",
    "analyze:size": "bundlesize",
    "analyze:deps": "npx depcheck",
    "dev:staging": "vite --mode staging",
    "build:staging": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode production",
    "test:e2e:ci": "start-server-and-test dev http://localhost:5173 'cypress run --headless'",
    "test:notifications": "vitest run src/components/notifications/__tests__/",
    "test:notifications:coverage": "vitest run --coverage src/components/notifications/__tests__/",
    "test:notifications:watch": "vitest watch src/components/notifications/__tests__/",
    "test:notifications:ui": "vitest --ui src/components/notifications/__tests__/",
    "test:cleanup": "npx tsx src/test/scripts/cleanupNotificationTests.ts"
  },
  "dependencies": {
