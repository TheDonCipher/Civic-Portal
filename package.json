    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.0.0",
    "@types/dompurify": "^3.0.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.1.5",
    "framer-motion": "^11.18.0",
